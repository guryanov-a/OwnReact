{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "jest"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "settings": {
    "react": {
      "pragma": "OwnReactComponent"
    }
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off"
  }
}
